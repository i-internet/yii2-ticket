{
    "name": "simialbi/yii2-ticket",
    "type": "yii2-extension",
    "description": "Ticketing system for yii2",
    "keywords": [
        "yii2",
        "ticket",
        "ticketing",
        "support"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simialbi",
            "email": "simi.albi@outlook.com"
        }
    ],
    "support": {
        "source": "https://github.com/simialbi/yii2-ticket",
        "issues": "https://github.com/simialbi/yii2-ticket/issues"
    },
    "minimum-stability": "beta",
    "require": {
        "php": ">=5.6",
        "rmrevin/yii2-fontawesome": "^3.4.0",
        "sandritsch91/yii2-widget-flatpickr": "^1.0.0",
        "bower-asset/autosize": "^4.0.2"
    },
    "require-dev": {
        "fortawesome/font-awesome": "^5.10.0",
        "phpunit/phpunit": "<7"
    },
    "suggest": {
        "simialbi/yii2-summernote": "Install this extension to use rich text fields in comment fields"
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\ticket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\extensions\\ticket\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
