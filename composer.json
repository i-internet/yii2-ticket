{
    "name": "simialbi/yii2-ticket",
    "type": "yii2-extension",
    "description": "Ticketing system for yii2",
    "keywords": [
        "yii2",
        "ticket",
        "ticketing",
        "support"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simialbi",
            "email": "simi.albi@outlook.com"
        }
    ],
    "support": {
        "source": "https://github.com/simialbi/yii2-ticket",
        "issues": "https://github.com/simialbi/yii2-ticket/issues"
    },
    "minimum-stability": "beta",
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "^2.0.20",
        "yiisoft/yii2-bootstrap4": "^2.0.4",
        "kartik-v/yii2-grid": "^3.3.0",
        "kartik-v/yii2-widget-select2": "^2.1.0",
        "simialbi/yii2-fontawesome": "^3.4.0",
        "simialbi/yii2-simialbi-base": ">=0.10.1 <1.0 | ^1.0.0",
        "simialbi/yii2-widget-datedropper": "^1.0.0",
        "bower-asset/autosize": "^4.0.2",
        "bower-asset/resumablejs": "^1.1.0"
    },
    "require-dev": {
        "fortawesome/font-awesome": "^5.10.0",
        "simialbi/yii2-kanban": "^1.0.0",
        "simialbi/yii2-summernote": "^1.1.0",
        "yiisoft/yii2-coding-standards": "~2.0",
        "phpunit/phpunit": "<7"
    },
    "suggest": {
        "simialbi/yii2-summernote": "Install this extension to use rich text fields in comment fields"
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\ticket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\extensions\\ticket\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
